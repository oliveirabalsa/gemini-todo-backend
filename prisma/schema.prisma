generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id          String    @id @default(uuid())
  title       String
  description String?
  priority    String?   @default("LOW")
  status      String?   @default("TODO")
  dueDate     DateTime?
}
